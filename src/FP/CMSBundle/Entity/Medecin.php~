<?php

namespace FP\CMSBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use FOS\UserBundle\Model\User as BaseUser;
use FP\CMSBundle\Entity\Service;
use FP\CMSBundle\Entity\Specialite;

/**
 * Medecin
 *
 * @ORM\Table(name="medecin")
 * @ORM\Entity(repositoryClass="FP\CMSBundle\Repository\MedecinRepository")
 */
class Medecin extends BaseUser
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var string
     *
     * @ORM\Column(name="prenom_medecin", type="string", length=100, nullable=true)
     */
    private $prenomMedecin;

    /**
     * @var string
     *
     * @ORM\Column(name="nom_medecin", type="string", length=100, nullable=true)
     */
    private $nomMedecin;

    /**
     * @var string
     *
     * @ORM\Column(name="genre_medecin", type="string", length=1, nullable=true)
     */
    private $genreMedecin;

    /**
     * @var string
     *
     * @ORM\Column(name="tel_medecin", type="string", length=15, nullable=true)
     */
    private $telMedecin;

    /**
     * @var bool
     * @ORM\Column(name="first_connexion", type="boolean", nullable=true)
     */
    private $firstConnexion;

	/**
     * @var CMSBundle\Entity\service
     *
     * @ORM\ManyToOne(targetEntity="FP\CMSBundle\Entity\Service")
     * @ORM\JoinColumn(nullable=true)
     */
    private $service;

    /**
     * @var CMSBundle\Entity\specialite
     *
     * @ORM\ManyToOne(targetEntity="FP\CMSBundle\Entity\Specialite")
     * @ORM\JoinColumn(nullable=true)
     */
    private $specialite;

    public function __construct()
    {
        parent::__construct();
        
    }


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set prenomMedecin
     *
     * @param string $prenomMedecin
     *
     * @return Medecin
     */
    public function setprenomMedecin($prenomMedecin)
    {
        $this->prenomMedecin = $prenomMedecin;

        return $this;
    }

    /**
     * Get prenomMedecin
     *
     * @return string
     */
    public function getprenomMedecin()
    {
        return $this->prenomMedecin;
    }

    /**
     * Set nomMedecin
     *
     * @param string $nomMedecin
     *
     * @return Medecin
     */
    public function setNomMedecin($nomMedecin)
    {
        $this->nomMedecin = $nomMedecin;

        return $this;
    }

    /**
     * Get nomMedecin
     *
     * @return string
     */
    public function getNomMedecin()
    {
        return $this->nomMedecin;
    }

    /**
     * Set genreMedecin
     *
     * @param string $genreMedecin
     *
     * @return Medecin
     */
    public function setGenreMedecin($genreMedecin)
    {
        $this->genreMedecin = $genreMedecin;

        return $this;
    }

    /**
     * Get genreMedecin
     *
     * @return string
     */
    public function getGenreMedecin()
    {
        return $this->genreMedecin;
    }

    /**
     * Set telMedecin
     *
     * @param string $telMedecin
     *
     * @return Medecin
     */
    public function setTelMedecin($telMedecin)
    {
        $this->telMedecin = $telMedecin;

        return $this;
    }

    /**
     * Get telMedecin
     *
     * @return string
     */
    public function getTelMedecin()
    {
        return $this->telMedecin;
    }

    /**
     * Set emailMedecin
     *
     * @param string $emailMedecin
     *
     * @return Medecin
     */
    public function setEmailMedecin($emailMedecin)
    {
        $this->emailMedecin = $emailMedecin;

        return $this;
    }

    /**
     * Get emailMedecin
     *
     * @return string
     */
    public function getEmailMedecin()
    {
        return $this->emailMedecin;
    }

    /**
     * Set service
     *
     * @param \FP\CMSBundle\Entity\Service $service
     *
     * @return Medecin
     */
    public function setService(\FP\CMSBundle\Entity\Service $service)
    {
        $this->service = $service;

        return $this;
    }

    /**
     * Get service
     *
     * @return \FP\CMSBundle\Entity\Service
     */
    public function getService()
    {
        return $this->service;
    }

    /**
     * Set specialite
     *
     * @param \FP\CMSBundle\Entity\Specialite $specialite
     *
     * @return Medecin
     */
    public function setSpecialite(\FP\CMSBundle\Entity\Specialite $specialite)
    {
        $this->specialite = $specialite;

        return $this;
    }

    /**
     * Get specialite
     *
     * @return \FP\CMSBundle\Entity\Specialite
     */
    public function getSpecialite()
    {
        return $this->specialite;
    }


    /**
     * Set firstConnexion
     *
     * @param boolean $firstConnexion
     *
     * @return Medecin
     */
    public function setFirstConnexion($firstConnexion)
    {
        $this->firstConnexion = $firstConnexion;

        return $this;
    }

    /**
     * Get firstConnexion
     *
     * @return boolean
     */
    public function getFirstConnexion()
    {
        return $this->firstConnexion;
    }
}
