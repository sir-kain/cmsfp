{% extends 'base.html.twig' %}

{% block body %}
    <h1>Diagnostic</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ diagnostic.id }}</td>
            </tr>
            <tr>
                <th>Forupdate</th>
                <td>{{ diagnostic.forupdate }}</td>
            </tr>
            <tr>
                <th>Chronique</th>
                <td>{{ diagnostic.chronique }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('diagnostic_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('diagnostic_edit', { 'id': diagnostic.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
